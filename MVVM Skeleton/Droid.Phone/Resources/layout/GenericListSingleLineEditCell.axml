<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:p1="http://schemas.android.com/apk/res/android"
    xmlns:custom="http://schemas.android.com/apk/res-auto"
    p1:layout_width="match_parent"
    p1:layout_height="wrap_content"
    p1:id="@+id/GenericListSingleLineEditCellMainLayout"
    p1:background="#FFFFFF"
    p1:paddingBottom="10dp"
    p1:paddingTop="10dp">
    <TextView
        p1:text="Dr. Martin Jameson, MD"
        p1:layout_width="match_parent"
        p1:layout_height="wrap_content"
        p1:id="@+id/GenericListSingleLineEditCellBody"
        p1:textSize="18sp"
        custom:customFont="Fonts/CenturyGothic.ttf"
        p1:layout_marginTop="3dp"
        p1:textColor="#555555"
        p1:layout_marginLeft="20dp"
        p1:layout_marginRight="20dp"
        p1:layout_centerVertical="true" />
    <ImageView
        p1:src="@drawable/Dots"
        p1:layout_width="wrap_content"
        p1:id="@+id/GetConnectedPromptListEditButton"
        p1:layout_alignParentRight="true"
        p1:layout_height="wrap_content"
        p1:paddingLeft="45dp"
        p1:paddingRight="10dp"
        p1:paddingTop="15dp"
        p1:scaleType="fitCenter"
        p1:paddingBottom="15dp" />
</RelativeLayout>