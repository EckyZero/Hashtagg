<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:p1="http://schemas.android.com/apk/res/android"
    xmlns:custom="http://schemas.android.com/apk/res-auto"
    p1:layout_width="match_parent"
    p1:layout_height="wrap_content"
    p1:background="#F7F3DE"
    p1:minWidth="25px"
    p1:minHeight="25px"
    p1:paddingBottom="10dp">
    <ProgressBar
        style="?android:attr/progressBarStyleHorizontal"
        p1:layout_width="150dp"
        p1:layout_height="10dp"
        p1:id="@+id/GetConnectedPromptListProgressBar"
        p1:layout_alignParentRight="true"
        p1:layout_alignParentTop="true"
        p1:progress="25"
        p1:layout_marginRight="75dp"
        p1:layout_marginTop="15dp"
        p1:progressDrawable="@drawable/ProgressBar" />
    <Droid.Controls.PSTextView
        p1:text="progress"
        p1:layout_width="wrap_content"
        p1:layout_height="wrap_content"
        p1:layout_toLeftOf="@id/GetConnectedPromptListProgressBar"
        p1:layout_marginRight="2dp"
        p1:id="@+id/GetConnectedPromptListProgressBarText"
        p1:textSize="12sp"
        custom:customFont="Fonts/Futura-Book.ttf"
        p1:textColor="#C5C5B9"
        p1:layout_marginTop="10dp"
        p1:layout_alignParentTop="true" />
    <ImageView
        p1:layout_centerInParent="true"
        p1:layout_width="wrap_content"
        p1:layout_height="70dp"
        p1:id="@+id/GetConnectedPromptListMainImage"
        p1:src="@drawable/Success"
        p1:layout_alignParentTop="true"
        p1:layout_marginTop="50dp" />
    <Droid.Controls.PSTextView
        p1:text="Great! Have any others?"
        p1:layout_width="wrap_content"
        p1:layout_height="wrap_content"
        p1:layout_below="@id/GetConnectedPromptListMainImage"
        p1:id="@+id/GetConnectedPromptListTitle"
        p1:layout_centerHorizontal="true"
        p1:textSize="28sp"
        p1:textColor="#555555"
        custom:customFont="Fonts/ShadowsIntoLightTwo-Regular.ttf"
        p1:gravity="center"
        p1:layout_alignParentLeft="true"
        p1:layout_alignParentRight="true"
        p1:layout_marginLeft="25dp"
        p1:layout_marginRight="25dp"
        p1:layout_marginTop="25dp" />
    <Droid.Controls.PSTextView
        p1:text="Dentist, Eye Doctor or any other specialists."
        p1:layout_width="wrap_content"
        p1:layout_height="wrap_content"
        p1:layout_below="@id/GetConnectedPromptListTitle"
        p1:gravity="center"
        p1:textSize="14sp"
        p1:textColor="#808080"
        custom:customFont="Fonts/CenturyGothic.ttf"
        p1:layout_alignParentLeft="true"
        p1:layout_alignParentRight="true"
        p1:id="@+id/GetConnectedPromptListDescription"
        p1:layout_marginTop="10dp"
        p1:layout_marginLeft="40dp"
        p1:layout_marginRight="40dp" />
</RelativeLayout>