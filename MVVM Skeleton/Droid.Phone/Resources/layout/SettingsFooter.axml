<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:p1="http://schemas.android.com/apk/res/android"
    xmlns:custom="http://schemas.android.com/apk/res-auto"
    p1:orientation="vertical"
    p1:minWidth="25px"
    p1:minHeight="25px"
    p1:layout_width="match_parent"
    p1:layout_height="wrap_content"
    p1:id="@+id/SettingsFooterLayout"
    p1:background="#ECE4B7"
    p1:paddingBottom="1dp">
    <RelativeLayout
        p1:id="@+id/SettingsFooterFamilyMembersInviteAnotherLayout"
        p1:background="#FFFFFF"
        p1:minWidth="25px"
        p1:minHeight="25px"
        p1:layout_width="match_parent"
        p1:layout_height="50dp"
        p1:layout_marginTop="1dp">
        <Droid.Controls.PSTextView
            p1:text="Invite a family member"
            p1:layout_width="wrap_content"
            p1:layout_height="wrap_content"
            p1:id="@+id/SettingsFooterFamilyMembersInviteAnotherDescriptor"
            p1:layout_centerInParent="true"
            custom:customFont="Fonts/Futura-Book.ttf"
            p1:textColor="#3D89CC"
            p1:textSize="14sp"
            p1:drawableLeft="@drawable/ic_Plus"
            p1:drawablePadding="15dp" />
    </RelativeLayout>
    <RelativeLayout
        p1:minWidth="25px"
        p1:minHeight="25px"
        p1:layout_width="match_parent"
        p1:layout_height="15dp"
        p1:id="@+id/SettingsHeaderFamilyMembersHeader"
        p1:paddingTop="10dp"
        p1:paddingLeft="25dp"
        p1:paddingBottom="5dp"
        p1:background="#F7F3DE"
        p1:layout_marginTop="1dp" />
    <RelativeLayout
        p1:minWidth="25px"
        p1:minHeight="25px"
        p1:layout_width="match_parent"
        p1:layout_height="50dp"
        p1:id="@+id/SettingsFooterSupportLayout"
        p1:background="#FFFFFF"
        p1:layout_marginTop="1dp">
        <Droid.Controls.PSTextView
            p1:text="Support"
            p1:layout_width="wrap_content"
            p1:layout_height="wrap_content"
            p1:id="@+id/SettingsFooterSupportDescriptor"
            p1:layout_centerVertical="true"
            p1:paddingLeft="20dp"
            custom:customFont="Fonts/CenturyGothic.ttf"
            p1:textColor="#3D89CC"
            p1:textSize="14sp" />
    </RelativeLayout>
    <RelativeLayout
        p1:minWidth="25px"
        p1:minHeight="25px"
        p1:layout_width="match_parent"
        p1:layout_height="50dp"
        p1:id="@+id/SettingsFooterLogoutLayout"
        p1:background="#FFFFFF"
        p1:layout_marginTop="1dp">
        <Droid.Controls.PSTextView
            p1:text="Logout"
            p1:layout_width="wrap_content"
            p1:layout_height="wrap_content"
            p1:id="@+id/SettingsFooterLogoutDescriptor"
            p1:layout_centerVertical="true"
            p1:paddingLeft="20dp"
            custom:customFont="Fonts/CenturyGothic.ttf"
            p1:textColor="#3D89CC"
            p1:textSize="14sp" />
    </RelativeLayout>
</LinearLayout>